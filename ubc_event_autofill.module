<?php

use Drupal\Core\Form\FormStateInterface;

/**
 * Implements hook_form_alter().
 */
function ubc_event_autofill_form_alter(&$form, FormStateInterface $form_state, $form_id) {
  if ($form_id == 'node_ubc_event_form' || $form_id == 'node_ubc_event_edit_form') {
    $form['#attached']['library'][] = 'ubc_event_autofill/autofill';
  }
}
